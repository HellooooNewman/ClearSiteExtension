{
  "manifest_version": 2,
  "name": "Clear Site",
  "description": "Clear site data from current site url. Cookies, localstorage, indexeddb, etc.",
  "version": "1.0.0",
  "icons" :{
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "browsingData"
  ]
}